    insert ignore into t_task_item(gmt_create, task_id, artifact_id, found_versions, dependent_name, status_code, order_status)
    SELECT now(), t.task_id, t.artifact_id, t.found_versions, d.simple_name, d.status_code, 'CREATE'
    FROM t_task_child t, t_task_info i, t_project_dependent d, t_project_info p
